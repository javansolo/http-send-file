{
  "name": "javansolo/http-send-file",
  "description": "PHP lib which sends a file with support for (multiple) range requests and throttle.",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "php",
    "http",
    "file",
    "download",
    "resume",
    "throttle"
  ],
  "require": {
    "php": ">=7.0",
    "ext-fileinfo": "*",
	"composer-runtime-api": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Javansolo\\HttpSendFile\\": "src/"
    }
  },
  "config": {
	"optimize-autoloader": true,
	"preferred-install": "dist",
	"sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
